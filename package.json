{
  "name": "altp6-flashcard_app-be",
  "version": "1.0.0",
  "description": "Flashcard App that an app with interactive flashcards that you can flip, sort, and mark as done",
  "main": "index.js",
  "repository": "https://github.com/mucyoalexaxel/ALTP6-Flashcard_App-be",
  "author": "Alex Axel Mucyo",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon",
    "start": "yarn prisma db push && node ./dist/src/index.js",
    "build": "prisma generate && yarn run generate && tsc -p tsconfig.json",
    "lint:fix": "eslint --fix .",
    "generate": "ts-node --transpile-only  -r tsconfig-paths/register src/graphql/schema/schema.ts",
    "generate:migration": "npx prisma migrate dev",
    "generate:reset": "npx prisma migrate reset --force",
    "prisma:studio": "npx prisma studio"
  },
  "devDependencies": {
    "@types/dotenv-safe": "^8.1.2",
    "@types/jest": "^26.0.24",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "eslint": "^7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-mock-extended": "^1.0.18",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "prisma": "^4.0.0",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "apollo-graphql": "^0.9.3",
    "apollo-server": "^3.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv-cli": "^4.0.0",
    "dotenv-safe": "^8.2.0",
    "express": "^4.18.1",
    "graphql": "^15.5.1",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1"
  }
}
